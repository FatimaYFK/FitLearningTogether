<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.dashboard">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dashboard">
        <activity android:name=".notification.Wecker"
            android:theme="@style/AppTheme.NoActionBar" />
        <receiver android:name=".notification.AlarmReceiver"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LoginDachboard" />
        <activity
            android:name=".tictactoe.Tictac"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".tictactoe.Afterstart"
            android:label="@string/title_activity_afterstart"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Yourprofil" />
        <activity android:name=".EditProfile" />
        <activity android:name=".Register" />
        <activity android:name=".Login" />
        <activity android:name=".All" />
        <activity android:name=".HTM" />
        <activity android:name=".Greenhouse" />
        <activity android:name=".weather.view.ui.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".weather.view.ui.ActivityWeather"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Profile" />
        <activity android:name=".calculator.Calculator"
            android:theme="@style/AppTheme"/>
        <activity android:name=".calculator.ui.calculator.CalculatorFragment"
            android:theme="@style/AppTheme"
            tools:ignore="Instantiatable" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>